{
  "type": "object",
  "title": "Configuration",
  "required": [
    "#api_secret",
    "api_client_id",
    "result_file_name",
    "incremental_output",
    "filter",
    "dimensions",
    "metrics"
  ],
  "properties": {
        "#api_secret": {
      "type": "string",
      "title": "API client secret",
      "format": "password",
      "propertyOrder": 1
    },
            "api_client_id": {
      "type": "string",
      "title": "API client ID",
      "propertyOrder": 2
    },
      "result_file_name": {
      "type": "string",
      "title": "Result table name",
        "description": "Result storage table name",
        "default": "report-data",
      "propertyOrder": 3
    },
     "incremental_output": {
      "type": "number",
      "enum": [
        0,
        1
      ],
      "options": {
        "enum_titles": [
          "Full Load",
          "Incremental Update"
        ]
      },
      "default": 1,
      "title": "Load type",
      "description": "If set to Incremental update, the result tables will be updated based on primary key consisting of all selected dimensions. Full load overwrites the destination table each time, with no primary keys.",
      "propertyOrder": 4
    },
    "filter": {
      "type": "object",
      "title": "Filter",
      "required": [
        "date_range",
        "client_ids"
      ],
      "description": "Report filter",
      "propertyOrder": 10,
      "properties": {
        "date_range": {
          "type": "object",
          "title": "Date",
          "format": "grid",
          "description": "Report date range. A maximum date range accessible through API is 397 days (13 months) from today.",
          "propertyOrder": 1,
          "required": [
            "from_date",
            "to_date"
          ],
          "properties": {
            "from_date": {
              "type": "string",
              "default": "1 week ago",
              "title": "From Date [including]",
              "description": "Date in YYYY-MM-DD format or dateparser string i.e. 5 days ago, 1 month ago, yesterday, etc.",
              "propertyOrder": 300
            },
            "to_date": {
              "type": "string",
              "title": "To date [including]",
              "default": "today",
              "description": "Date in YYYY-MM-DD format or dateparser string i.e. 5 days ago, 1 month ago, yesterday, etc.",
              "propertyOrder": 350
            }
          }
        },
        "client_ids": {
          "type": "array",
          "title": "Client IDS",
          "description": "List of Client IDs. If left empty, all are downloaded",
          "items": {
            "type": "string",
            "title": "Client ID"
          }
        }
      }
    },
    "dimensions": {
      "type": "array",
